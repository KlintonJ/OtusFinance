<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OtusFinance.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OtusFinance.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="OtusFinance">

    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:LandingPage}"
        Route="MainPage" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="LoginPage" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:SignupPage}"
        Route="SignupPage" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:AccountSettings}"
        Route="AccountSettings" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:AccountReport}"
        Route="AccountReport" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:OverviewPage}"
        Route="OverviewPage" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:DashboardPage}"
        Route="DashboardPage" />

</Shell>
